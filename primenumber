#include<stdio.h>

int main()
{
  int n=4;
  int isPrime=1;  //assume num is prime
  for(int i=2;i*i<n;i++)    // i*i beacause we need root n which we cant write                                     so we use i*i
    {
      if(n%i==0)
      {
        isPrime=0;
      }
    }
  if(isPrime==1)
  {
    printf("the %d is prime ",n);
  }else{
    printf("the %d is not  prime ",n);
  }
  return 0;
}

